                                                                <td>@rowSelect</td>
                                                                <td>@item[1]</td>
                                                                <td>
                                                                    <select name="matching" class="select">
                                                                        <option>กรุณาเลือกอาจารย์ที่ต้องการ</option>
                                                                        @foreach (DataRow teacher in Model.Tables[1].Rows)
                                                                        {
                                                                            <option value="@teacher[0]-@item[0]">@teacher[1]</option>
                                                                        }
                                                                    </select>
                                                                </td>
                                                            </tr>
                                                        }
