                                                            <th class="border-top-0">ชื่อสถานประกอบการ</th>
                                                            <th class="border-top-0">ชื่อ-นามสกุล</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (DataRow item in Model.Tables[0].Rows)
