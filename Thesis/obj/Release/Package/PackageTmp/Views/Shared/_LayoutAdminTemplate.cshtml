
@{
    if (Session["position"].ToString() != "ผู้ดูแล")
    {
        Response.Redirect("~/Home/LoginTemplate");
    }
}

<!DOCTYPE html>
<html dir="ltr">
<head>
    <meta charset="utf8mb4_general_ci">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
