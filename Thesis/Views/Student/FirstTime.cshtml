    @using (Html.BeginForm("FirstTime", "Student", FormMethod.Post))
    {
        <div class="container">
            <div class="form-horizontal">
                <div class="row">
                    <div class="form-group col-md-12" style="text-align:center; font-size:20px;">
                        @Html.Label("การเข้าใช้งานครั้งแรก", htmlAttributes: new { @class = "control-label" })
                        <hr />
                    </div>
                    <div class="form-group col-md-12" style="margin-top:15px; font-size:16px;">
                        @Html.Label("ข้อมูลส่วนตัวนักศึกษา:", htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="form-group col-md-12">
                        <div class="col-md-4">
                            @Html.Label("รหัสนักศึกษา:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.student_id, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>
                        <div class="col-md-4">
                            @Html.Label("ชื่อภาษาไทย:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.name_th, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>
                        <div class="col-md-4">
                            @Html.Label("ชื่อภาษาอังกฤษ:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.name_en, new { htmlAttributes = new { @class = "form-control", @maxlength = "70", @autocomplete = "off" } })
                            <span class="field-validation-valid text-danger" data-valmsg-for="name_en" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                    <div class="form-group col-md-12">
                        <div class="col-md-4">
                            @Html.Label("ภาควิชา:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.major, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>
                        <div class="col-md-2">
                            @Html.Label("นักศึกษาชั้นปีที่:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.year, new { htmlAttributes = new { @class = "form-control", @maxlength = "5", @autocomplete = "off", @type = "number" } })
                        </div>
                        <div class="col-md-4">
                            @Html.Label("อาจารย์ที่ปรึกษา:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.advisor, new { htmlAttributes = new { @class = "form-control", @maxlength = "70", @autocomplete = "off" } })
                        </div>
                    </div>
                    <div class="form-group col-md-12">
                        <div class="col-md-3">
                            @Html.Label("เกรดเฉลี่ยภาคการศึกษาที่ผ่านมา:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.gpa, new { htmlAttributes = new { @class = "form-control", @maxlength = "5", @autocomplete = "off", @type = "number" } })
                        </div>
                        <div class="col-md-3">
                            @Html.Label("เกรดเฉลี่ยรวม:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.gpax, new { htmlAttributes = new { @class = "form-control", @maxlength = "5", @autocomplete = "off", @type = "number" } })
                        </div>
                        <div class="col-md-offset-1 col-md-3" style="margin-top:25px; font-weight:bold;">
                            <label class="radio-inline control-label" style="font-weight:bold;">
                                <input type="radio" name="study" value="ภาคปกติ" checked>ภาคปกติ
                            </label>
                            <label class="radio-inline control-label" style="font-weight:bold; margin-left:40px;">
                                <input type="radio" name="study" value="ภาคพิเศษ">ภาคพิเศษ
                            </label>
                        </div>
                    </div>
                    <div class="form-group col-md-12">
                        <div class="col-md-3">
                            @Html.Label("บัตรประจำตัวประชาชนเลขที่:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.identification, new { htmlAttributes = new { @class = "form-control", @maxlength = "13", @autocomplete = "off" } })
                        </div>
                        <div class="col-md-3">
                            @Html.Label("ออกให้ ณ:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.at, new { htmlAttributes = new { @class = "form-control", @maxlength = "30", @autocomplete = "off" } })
                        </div>
                        <div class="col-md-2">
                            @Html.Label("เมื่อวันที่:", htmlAttributes: new { @class = "control-label" })
                            <div class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                                @Html.EditorFor(model => model.date, new { htmlAttributes = new { @class = "form-control datepicker", @autocomplete = "off" } })
                            </div>
                        </div>
                        <div class="col-md-2">
                            @Html.Label("เชื้อชาติ:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.race, new { htmlAttributes = new { @class = "form-control", @maxlength = "20", @autocomplete = "off" } })
                        </div>
                        <div class="col-md-2">
                            @Html.Label("สัญชาติ:", htmlAttributes: new { @class = "control-label" })
                            @Html.EditorFor(model => model.nationality, new { htmlAttributes = new { @class = "form-control", @maxlength = "20", @autocomplete = "off" } })
                        </div>
                    </div>
