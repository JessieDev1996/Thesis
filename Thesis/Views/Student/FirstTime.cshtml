    <script type="text/javascript">
        $(document).ready(function () {
            $('.datepicker').datepicker({
                autoclose: true,
                language: 'th',
                format: 'dd/mm/yyyy',
                orientation: "bottom auto"
            });

            $('#family').change(function () {
                var num = $(this).val();
                var gf = parseInt($("#get_family").val());

                $("#get_family").val(num);
                $("#result_family").val(num);

                if ((num - gf) > 0) {
                    for (var i = 0; i < (num - gf); i++) {
                        var html = '<tr id="' + (gf + i + 1) + '">';
                        html += '<td><label class="control-label" style="font-weight:normal">' + (gf + i + 1) + '</label></td>';
                        html += '<td><input class="form-control" type="text" name="fa_name[' + (gf + i + 3) + ']" maxlength="70" autocomplete="off"></td>';
                        html += '<td><input class="form-control" type="number" name="fa_age[' + (gf + i + 2) + ']" maxlength="5" autocomplete="off"></td>';
                        html += '<td><input class="form-control" type="text" name="fa_occupation[' + (gf + i + 3) + ']" maxlength="50" autocomplete="off"></td>';
                        html += '<td><input class="form-control" type="text" name="fa_position[' + (gf + i) + ']" maxlength="50" autocomplete="off"></td>';
                        html += '<td><input class="form-control" type="text" name="fa_place_work[' + (gf + i + 1) + ']" maxlength="70" autocomplete="off"></td>';
                        html += '</tr>';
                        $('#add_family').append(html);
                    }

                } else if ((num - gf) < 0) {
                    for (var i = 0; i < (gf - num); i++) {
                        $('table#dynamic_family tr#' + (gf - i).toString()).remove();
                    }
                }

            });

            $(document).on('click', '.add', function () {
                var lg = parseInt($("#language").val());
                lg = lg + 1;
                $("#language").val(lg);

                var html = '<tr id="form_language">';
                html += '<td>';
                html += '<input class="form-control" type="text" name="la_name[' + (lg - 1) + ']" maxlength="20" autocomplete="off">';
                html += '</td >';
                html += '<td>';
                html += '<input type="radio" name="la_listening[' + (lg - 1) + ']">';
                html += '<input type="radio" name="la_listening[' + (lg - 1) + ']" style="margin:0 25px 0 25px;">';
                html += '<input type="radio" name="la_listening[' + (lg - 1) + ']">';
                html += '</td>';
                html += '<td>';
                html += '<input type="radio" name="la_speaking[' + (lg - 1) + ']">';
                html += '<input type="radio" name="la_speaking[' + (lg - 1) + ']" style="margin:0 25px 0 25px;">';
                html += '<input type="radio" name="la_speaking[' + (lg - 1) + ']">';
                html += '</td>';
                html += '<td>';
                html += '<input type="radio" name="la_reading[' + (lg - 1) + ']">';
                html += '<input type="radio" name="la_reading[' + (lg - 1) + ']" style="margin:0 25px 0 25px;">';
                html += '<input type="radio" name="la_reading[' + (lg - 1) + ']">';
                html += '</td>';
                html += '<td>';
                html += '<input type="radio" name="la_writing[' + (lg - 1) + ']">';
                html += '<input type="radio" name="la_writing[' + (lg - 1) + ']" style="margin:0 25px 0 25px;">';
                html += '<input type="radio" name="la_writing[' + (lg - 1) + ']">';
                html += '</td>';
                html += '<th class="col-md-1">';
                html += '<button type="button" id = "btn-op' + (lg - 1) + '" class="btn btn-danger remove">ลบ</button>';
                html += '</th>';
                html += '</tr>';

                $('#add_language').append(html);

                $("#btn-op" + (lg - 2).toString()).hide();
            });

            $(document).on('click', '.remove', function () {
                var lg = parseInt($("#language").val());
                lg = lg - 1;
                $("#language").val(lg)

                $(this).closest('#form_language').remove();

                $("#btn-op" + (lg - 1).toString()).show();
            });

            $("#province").change(function () {
                var Area = {};
                Area.url = "/Student/Area";
                Area.type = "POST";
                Area.data = JSON.stringify({
                    province: $("#province").val()
                });
                Area.datatype = "json";
                Area.contentType = "application/json";
                Area.success = function (AreaList) {
                    $('.selectpicker').selectpicker('render');
                    $("#area").empty();
                    $("#sub_area").empty();
                    $('.selectpicker').selectpicker('refresh');
                    $('.selectpicker').selectpicker('render');
                    $("#area").append("<option hidden disabled selected>กรุณาเลือกอำเภอ</option>");
                    $("#sub_area").append("<option hidden disabled selected>กรุณาเลือกตำบล</option>");
                    $('.selectpicker').selectpicker('refresh');
                    for (var i = 0; i < AreaList.length; i++) {
                        $('.selectpicker').selectpicker('render');
                        $("#area").append("<option>" + AreaList[i] + "</option>");
                        $('.selectpicker').selectpicker('refresh');
                    }
